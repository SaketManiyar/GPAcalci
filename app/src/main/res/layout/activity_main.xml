<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTintMode="add"
    tools:context=".MainActivity">


    <TextView
        android:id="@+id/textView"
        android:layout_width="90dp"
        android:layout_height="35dp"
        android:layout_marginTop="40dp"
        android:text="@string/credit"
        android:textColor="#FFFFFF"
        app:autoSizeTextType="uniform"
        app:layout_constraintEnd_toEndOf="@+id/c1"
        app:layout_constraintStart_toStartOf="@+id/c1"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="90dp"
        android:layout_height="35dp"
        android:layout_marginTop="40dp"
        android:text="@string/grade"
        android:textColor="#FFFFFF"
        app:autoSizeTextType="uniform"
        app:layout_constraintEnd_toEndOf="@+id/s1"
        app:layout_constraintStart_toStartOf="@+id/s1"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button2"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="28dp"
        android:background="@drawable/edit_round1"
        android:onClick="reset"
        android:text="@string/reset"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="OnClick" />

    <Button
        android:id="@+id/button"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="28dp"
        android:background="@drawable/edit_round1"
        android:onClick="result"
        android:text="@string/result"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5" />

    <Spinner
        android:id="@+id/s1"
        android:layout_width="125dp"
        android:layout_height="37dp"
        android:layout_marginTop="120dp"
        android:background="@drawable/edit_round"
        android:paddingStart="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/guideline"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints,RtlSymmetry" />

    <Spinner
        android:id="@+id/s6"
        android:layout_width="125dp"
        android:layout_height="37dp"
        android:layout_marginTop="22dp"
        android:background="@drawable/edit_round"
        android:paddingStart="8dp"
        app:layout_constraintEnd_toEndOf="@+id/s1"
        app:layout_constraintStart_toStartOf="@+id/s1"
        app:layout_constraintTop_toBottomOf="@+id/s5"
        tools:ignore="RtlSymmetry" />

    <Spinner
        android:id="@+id/s8"
        android:layout_width="125dp"
        android:layout_height="37dp"
        android:layout_marginTop="22dp"
        android:background="@drawable/edit_round"
        android:paddingStart="8dp"
        app:layout_constraintEnd_toEndOf="@+id/s1"
        app:layout_constraintStart_toStartOf="@+id/s1"
        app:layout_constraintTop_toBottomOf="@+id/s7"
        tools:ignore="RtlSymmetry" />

    <Spinner
        android:id="@+id/s7"
        android:layout_width="125dp"
        android:layout_height="37dp"
        android:layout_marginTop="22dp"
        android:background="@drawable/edit_round"
        android:paddingStart="8dp"
        app:layout_constraintEnd_toEndOf="@+id/s1"
        app:layout_constraintStart_toStartOf="@+id/s1"
        app:layout_constraintTop_toBottomOf="@+id/s6"
        tools:ignore="RtlSymmetry" />

    <Spinner
        android:id="@+id/s5"
        android:layout_width="125dp"
        android:layout_height="37dp"
        android:layout_marginTop="22dp"
        android:background="@drawable/edit_round"
        android:paddingStart="8dp"
        app:layout_constraintEnd_toEndOf="@+id/s1"
        app:layout_constraintStart_toStartOf="@+id/s1"
        app:layout_constraintTop_toBottomOf="@+id/s4"
        tools:ignore="RtlSymmetry" />

    <Spinner
        android:id="@+id/s4"
        android:layout_width="125dp"
        android:layout_height="37dp"
        android:layout_marginTop="22dp"
        android:background="@drawable/edit_round"
        android:paddingStart="8dp"
        app:layout_constraintEnd_toEndOf="@+id/s1"
        app:layout_constraintStart_toStartOf="@+id/s1"
        app:layout_constraintTop_toBottomOf="@+id/s3"
        tools:ignore="RtlSymmetry" />

    <Spinner
        android:id="@+id/s3"
        android:layout_width="125dp"
        android:layout_height="37dp"
        android:layout_marginTop="22dp"
        android:background="@drawable/edit_round"
        android:paddingStart="8dp"
        app:layout_constraintEnd_toEndOf="@+id/s1"
        app:layout_constraintStart_toStartOf="@+id/s1"
        app:layout_constraintTop_toBottomOf="@+id/s2"
        tools:ignore="RtlSymmetry" />

    <Spinner
        android:id="@+id/s2"
        android:layout_width="125dp"
        android:layout_height="37dp"
        android:layout_marginTop="22dp"
        android:background="@drawable/edit_round"
        android:paddingStart="8dp"
        app:layout_constraintEnd_toEndOf="@+id/s1"
        app:layout_constraintStart_toStartOf="@+id/s1"
        app:layout_constraintTop_toBottomOf="@+id/s1"
        tools:ignore="RtlSymmetry" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="130dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Spinner
        android:id="@+id/c1"
        android:layout_width="125dp"
        android:layout_height="37dp"
        android:layout_marginTop="120dp"
        android:background="@drawable/edit_round"
        android:paddingStart="8dp"
        app:layout_constraintEnd_toStartOf="@+id/guideline"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="RtlSymmetry" />

    <Spinner
        android:id="@+id/c2"
        android:layout_width="125dp"
        android:layout_height="37dp"
        android:layout_marginTop="22dp"
        android:background="@drawable/edit_round"
        android:paddingStart="8dp"
        app:layout_constraintEnd_toEndOf="@+id/c1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/c1"
        app:layout_constraintTop_toBottomOf="@+id/c1"
        tools:ignore="RtlSymmetry" />

    <Spinner
        android:id="@+id/c3"
        android:layout_width="125dp"
        android:layout_height="37dp"
        android:layout_marginTop="22dp"
        android:background="@drawable/edit_round"
        android:paddingStart="8dp"
        app:layout_constraintEnd_toEndOf="@+id/c2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/c2"
        app:layout_constraintTop_toBottomOf="@+id/c2"
        tools:ignore="RtlSymmetry" />

    <Spinner
        android:id="@+id/c4"
        android:layout_width="125dp"
        android:layout_height="37dp"
        android:layout_marginTop="22dp"
        android:background="@drawable/edit_round"
        android:paddingStart="8dp"
        app:layout_constraintEnd_toEndOf="@+id/c3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/c3"
        app:layout_constraintTop_toBottomOf="@+id/c3"
        tools:ignore="RtlSymmetry" />

    <Spinner
        android:id="@+id/c5"
        android:layout_width="125dp"
        android:layout_height="37dp"
        android:layout_marginTop="22dp"
        android:background="@drawable/edit_round"
        android:paddingStart="8dp"
        app:layout_constraintEnd_toEndOf="@+id/c4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/c4"
        app:layout_constraintTop_toBottomOf="@+id/c4"
        tools:ignore="RtlSymmetry" />

    <Spinner
        android:id="@+id/c6"
        android:layout_width="125dp"
        android:layout_height="37dp"
        android:layout_marginTop="22dp"
        android:background="@drawable/edit_round"
        android:paddingStart="8dp"
        app:layout_constraintEnd_toEndOf="@+id/c5"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/c5"
        app:layout_constraintTop_toBottomOf="@+id/c5"
        tools:ignore="RtlSymmetry" />

    <Spinner
        android:id="@+id/c7"
        android:layout_width="125dp"
        android:layout_height="37dp"
        android:layout_marginTop="22dp"
        android:background="@drawable/edit_round"
        android:paddingStart="8dp"
        app:layout_constraintEnd_toEndOf="@+id/c6"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/c6"
        app:layout_constraintTop_toBottomOf="@+id/c6"
        tools:ignore="RtlSymmetry" />

    <Spinner
        android:id="@+id/c8"
        android:layout_width="125dp"
        android:layout_height="37dp"
        android:layout_marginTop="22dp"
        android:background="@drawable/edit_round"
        android:paddingStart="8dp"
        app:layout_constraintEnd_toEndOf="@+id/c7"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/c7"
        app:layout_constraintTop_toBottomOf="@+id/c7"
        tools:ignore="RtlSymmetry" />


</androidx.constraintlayout.widget.ConstraintLayout>

